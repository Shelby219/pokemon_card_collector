extends layout

block content

 div.title
  h1.title-text= title
  div  
    a.button(href=`/pokemon/create`) Add to Collection
  
  ul
  .pokemon-container
    each poke in pokes
      li.pokemon
       div.pokemon-div
        p.pokemon-p=poke.name
        img(src=`${poke.image}`).pokemon-p
        p.pokemon-p=poke.type
     
       
